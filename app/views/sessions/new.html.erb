<%= form_tag('/signin', method: :post) do %>

    <%= label_tag(:name, "Name:") %><br>
    <%= collection_select(:user, :name, User.all, :name, :name) %><br><br>
    
    <%= label_tag(:password, "Password:") %><br>
    <%= password_field_tag(:password) %><br><br>
    
    <%= submit_tag("Sign In") %>

<% end %> 
